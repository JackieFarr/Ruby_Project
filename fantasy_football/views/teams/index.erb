<h1>These are the teams</h1>

<p class="btn--wrapper">
  <a class="btn btn--action" href="/teams/new">Add a team</a>
</p>

<table>
  <tr>
    <th>Team Name</th>
    <th>Team Players</th>
  </tr>
    <% for team in @teams %>
  <tr>
    <td><%= team.name %></td>
    <td> <p class="btn--wrapper">
         <a class="btn btn--action" href="/teams/<%= team.id %>">Show team players</a></p></td>
  </tr>
  <% end %>
</table>
